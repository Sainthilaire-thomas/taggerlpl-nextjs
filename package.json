{
  "name": "taggerlpl-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:whisper": "jest OpenAIWhisperProvider",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.3.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@mui/material-nextjs": "^7.0.2",
    "@mui/x-tree-view": "^8.0.0",
    "@supabase/supabase-js": "^2.49.4",
    "@types/jspdf": "^2.0.0",
    "echarts": "^5.6.0",
    "jspdf": "^3.0.1",
    "next": "15.3.1",
    "openai": "^4.28.0",
    "react": "^19.0.0",
    "react-color": "^2.19.3",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@welldone-software/why-did-you-render": "^10.0.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.1.0",
    "typescript": "^5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest.setup.js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|/__test__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "verbose": true
  }
}
